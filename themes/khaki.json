{
	"name": "Khaki",
	"tokenColors": [
		{
			"settings": {
				"background": "#D7D7AF",
				"foreground": "#5F5F00"
			}
		},
		{
			"scope": ["meta.embedded", "source.groovy.embedded"],
			"settings": {
				"background": "#D7D7AF",
				"foreground": "#5F5F00"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#57572F"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#005F5F"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#0087AF"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#005F00"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other",
				"punctuation.definition.variable"
			],
			"settings": {
				"foreground": "#000087"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"support.function.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#87005F"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#87005F"
			}
		},
		{
			"name": "Function & Class name",
			"scope": [
				"entity.name.function",
				"support.function",
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#4F4F00"
			}
		},
		{
			"name": "Embedded code markers",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#AF5F00"
			}
		},
		{
			"name": "Built-in constant",
			"scope": [
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#87005F"
			}
		},
		{
			"name": "User-defined constant",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#005F5F"
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#000087"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#5F5F00"
			}
		},
		{
			"name": "Continuation",
			"scope": "punctuation.separator.continuation",
			"settings": {
				"foreground": "#00ff00"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#5F5F00"
			}
		},
		{
			"name": "Library Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#000087"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#870000"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#005F5F"
			}
		}
	],
